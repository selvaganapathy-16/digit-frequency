#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int i,j,a,count=0;
char s[100];
int x[10],m;
scanf("%s",s);
for(i=0;i<10;i++)
{
    x[i]=0;
}
 for(i=0;i<strlen(s);i++)
 {
     m=s[i]-48;
    if(m>=0 && m<=9)
        {
           x[m]++;
        }
   
} 
for(i=0;i<10;i++)
{
    printf("%d ",x[i]);
}
    return 0;
}
